		cocktailviewerwidgetbase.h

cocktailviewerwidgetbase.h: cocktailviewerwidgetbase.ui 
	$(UIC) cocktailviewerwidgetbase.ui -o cocktailviewerwidgetbase.h

cocktailviewerwidgetbase.cpp: cocktailviewerwidgetbase.h cocktailviewerwidgetbase.ui 
	$(UIC) cocktailviewerwidgetbase.ui -i cocktailviewerwidgetbase.h -o cocktailviewerwidgetbase.cpp

cocktailviewerwidgetbase.o: cocktailviewerwidgetbase.cpp cocktailviewerwidgetbase.h

moc_cocktailviewer.o: moc_cocktailviewer.cpp  cocktailviewer.h 

moc_cocktailviewerwidget.o: moc_cocktailviewerwidget.cpp  cocktailviewerwidget.h cocktailviewerwidgetbase.h

moc_cocktailviewerwidgetbase.o: moc_cocktailviewerwidgetbase.cpp  cocktailviewerwidgetbase.h 

